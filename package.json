{
  "name": "rektrace",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=9"
  },
  "optionalDependencies": {
    "ws": "^8.17.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "rugscan:dev": "tsx watch rektrace-rugscan/src/index.ts",
    "dev:demo": "DOTENV_CONFIG_PATH=.env.demo tsx watch rektrace-rugscan/src/index.ts",
    "dev:local": "DOTENV_CONFIG_PATH=.env.local tsx watch rektrace-rugscan/src/index.ts",
    "build": "tsc -p .",
    "rugscan:build": "tsc -p rektrace-rugscan/tsconfig.json",
    "build:rugscan": "tsc -p rektrace-rugscan/tsconfig.json",
    "start": "node dist/index.js",
    "start:rugscan": "node dist/rektrace-rugscan/rektrace-rugscan/src/index.js",
    "test": "jest --runInBand",
    "test:all": "jest --runInBand",
    "docs:check": "tsx tools/check-docs.ts",
    "docs:open": "echo 'Open docs in Cursor preview'",
    "canary:node": "node tools/canary_node.cjs",
    "ip:public": "node tools/get_public_ip.mjs",
    "env:gen": "node tools/gen_env_from_ops.mjs ops/secrets.local.json .env.prod",
    "env:gen:live": "PRESET=live node tools/gen_env_from_ops.mjs ops/secrets.local.json .env.prod",
    "env:gen:demo": "PRESET=demo node tools/gen_env_from_ops.mjs ops/secrets.local.json .env.prod",
    "deploy:live": "bash scripts/deploy_live.sh",
    "smoke:live": "bash scripts/smoke_live.sh",
    "typecheck": "tsc -p . --noEmit",
    "preflight": "bash scripts/preflight.sh",
    "smoke:abuse": "bash scripts/smoke_abuse.sh",
    "oracle:scan": "bash scripts/oracle_scan.sh",
    "fuzz:scan": "bash scripts/fuzz_scan.sh",
    "chaos:smoke": "bash scripts/chaos_smoke.sh",
    "soak:scan": "bash scripts/soak_scan.sh",
    "git:hooks": "bash tools/install_hooks.sh",
    "canary:live": "bash scripts/canary_live.sh",
    "alerts:watch": "bash scripts/alerts_watch.sh",
    "synthetic:probe": "bash scripts/synthetic_probe.sh",
    "config:snapshot": "bash scripts/config_snapshot.sh",
    "logs:scrub": "bash scripts/log_scrub.sh",
    "env:lint": "bash scripts/env_lint.sh",
    "release:bundle": "bash scripts/release_bundle.sh",
    "signals:backtest": "bash scripts/signals_backtest.sh",
    "rehearse:signals": "bash scripts/rehearse_signals.sh",
    "rehearse:ws": "bash scripts/rehearse_ws.sh",
    "verify": "pnpm run build && pnpm run test && pnpm run env:lint && pnpm run preflight && pnpm run oracle:scan && pnpm run fuzz:scan && pnpm run chaos:smoke && bash scripts/sbom.sh",
    "release:checksum": "shasum -a 256 release_artifacts/*.tar.gz || sha256sum release_artifacts/*.tar.gz",
    "release:tag": "node tools/bump_and_tag.mjs",
    "ci:beta": "npm run test && bash scripts/smoke_mvp.sh || exit 1",
    "signoff": "bash scripts/signoff.sh",
    "package:rugscan": "bash scripts/package_rugscan.sh",
    "verify:pm2": "bash scripts/verify_pm2_target.sh",
    "go:live": "bash ops/go_live.sh",
    "watch:post": "bash ops/watch_post_launch.sh",
    "rollback:last": "bash ops/rollback_last.sh",
    "perf:baseline": "bash scripts/baseline_perf.sh",
    "perf:gate": "bash scripts/perf_gate.sh",
    "rehearse:launch": "bash ops/rehearse_launch.sh",
    "drill:freeze": "bash ops/drill_freeze.sh",
    "rules:flip": "bash ops/flip_rules.sh",
    "release:audit": "bash scripts/audit_pack.sh",
    "build:mcp": "tsc -b mcp/process-sentinel mcp/git-tools mcp/http-probe mcp/prom-metrics mcp/postgres-ro mcp/tg-notify",
    "start:sentinel": "tsx mcp/process-sentinel/server.ts",
    "start:git": "tsx mcp/git-tools/server.ts",
    "start:http": "tsx mcp/http-probe/server.ts",
    "cpp:configure": "cmake --preset default",
    "cpp:build": "cmake --build --preset default",
    "cpp:test": "ctest --test-dir build --output-on-failure",
    "cpp:run": "node tools/cpp_bridge.mjs demo",
    "cpp:bench": "node tools/cpp_bridge.mjs bench",
    "cpp:score": "node tools/score_bridge.mjs --holders 800 --lp_locked yes --risk 0.10",
    "cpp:score:low": "node tools/score_bridge.mjs --holders 120 --lp_locked no --risk 0.05"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@polkadot/api": "^10.12.1",
    "bullmq": "^5.56.10",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "fastify": "^5.5.0",
    "grammy": "^1.25.3",
    "ioredis": "^5.4.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "undici": "^6.19.8",
    "viem": "^2.33.3",
    "vscode-jsonrpc": "^8.2.0"
  },
  "devDependencies": {
    "@napi-rs/canvas": "^0.1.53",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.16",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
